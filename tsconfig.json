{
    "compilerOptions": {
        "module": "commonjs",
		"target": "es5",
		"lib": [
			"es2015",
			"esnext.asynciterable"
		],
        "declaration": true,
		"outDir": "./dist",
		"strict": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"types": [
			"jest"
		],
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"src/**/*"
	]
}
